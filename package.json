    "mustache": "^4.2.0",
    "obsidian": "^1.5.7-1",
    "simple-node-logger": "^21.8.12",
    "tar": "^7.4.0",
    "yazl": "^2.5.1"
  },
  "devDependencies": {
    "@eslint-community/eslint-utils": "^4.4.0",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.7",
